#include <iostream>

#define ll long long

using std::cin;
using std::cout;

int main()
{
    int N, M;
    cin >> N >> M;
    ll A[N][M];
    for (int i = 0; i < N; i++)
        for (int j = 0; j < M; j++)
            cin >> A[i][j];
    for (int i = 0; i < N; i++)
    {
        for (int j = M - 1; j >= 0; j--)
            cout << A[i][j] << " ";
        cout << "\n";
    }

    return 0;
}
