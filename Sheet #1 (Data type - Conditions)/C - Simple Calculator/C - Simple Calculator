#include <iostream>
#include <string>

using std::cin;
using std::cout;

int main()
{
    long long X, Y;
    cin >> X >> Y;
    cout << X << " + " << Y << " = " << X + Y << "\n";
    cout << X << " * " << Y << " = " << X * Y << "\n";
    cout << X << " - " << Y << " = " << X - Y << "\n";
    return 0;
}
