#include <iostream>
#include <cmath>

using std::cin;
using std::cout;
using std::log;

int main()
{
    double A, B, C, D;
    cin >> A >> B >> C >> D;
    double X = B * log(A);
    double Y = D * log(C);
    if (X > Y)
    {
        cout << "YES";
    }
    else
    {
        cout << "NO";
    }
    return 0;
}
