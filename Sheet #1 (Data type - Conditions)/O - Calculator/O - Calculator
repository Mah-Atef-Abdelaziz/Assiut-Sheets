#include <iostream>

using std::cin;
using std::cout;

int main()
{
    int A, B;
    char S;
    cin >> A >> S >> B;
    if (S == '+')
    {
        cout << A + B;
    }
    else if (S == '-')
    {
        cout << A - B;
    }
    else if (S == '*')
    {
        cout << A * B;
    }
    else if (S == '/')
    {
        cout << A / B;
    }
    else
    {
        cout << "Invalid Operation";
    }
    return 0;
}
