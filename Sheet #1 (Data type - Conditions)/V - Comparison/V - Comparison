#include <iostream>

using std::cin;
using std::cout;

int main()
{
    int A, B;
    char S;
    cin >> A >> S >> B;
    if (S == '<' && A < B)
    {
        cout << "Right";
    }
    else if (S == '>' && A > B)
    {
        cout << "Right";
    }
    else if (S == '=' && A == B)
    {
        cout << "Right";
    }
    else
    {
        cout << "Wrong";
    }
    return 0;
}
