#include <iostream>
#include <cmath>

using std::cin;
using std::cout;
using std::log;

int main()
{
    long long a, b, c, d;
    cin >> a >> b >> c >> d;
    if (a * b - c == d || a * b + c == d){
        cout << "YES";
    }
    else if (a + b * c == d || a - b * c == d){
        cout << "YES";
    }
    else if (a + b - c == d || a - b + c == d){
        cout << "YES";
    }
    else{
        cout << "NO";
    }
    return 0;
}
