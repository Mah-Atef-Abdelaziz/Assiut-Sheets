#include <iostream>

using std::cin;
using std::cout;

int main()
{
    char c;
    cin >> c;
    if (int(c) == 122)
    {
        cout << "a";
    }
    else if (int(c) == 90)
    {
        cout << "A";
    }
    else
    {
        int next = int(c) + 1;
        cout << char(next);
    }

    return 0;
}
