#include <iostream>

using std::cin;
using std::cout;

int main()
{
    int X;
    cin >> X;
    bool isPrime = true;
    for (int i = 2; i < X; i++)
    {
        if (X % i == 0)
        {
            isPrime = false;
        }
    }
    if (X != 1 && isPrime)
    {
        cout << "YES";
    }
    else
    {
        cout << "NO";
    }

    return 0;
}
