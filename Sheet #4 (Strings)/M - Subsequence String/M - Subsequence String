#include <iostream>
#include <algorithm>
#include <string>

using std::cin;
using std::cout;
using std::string;

int main()
{
    string S, test = "hello", temp;
    cin >> S;
    int index = 0;
    for (int i = 0; i < S.size(); i++)
    {
        if (S[i] == test[index])
        {
            temp.push_back(S[i]);
            index++;
        }
    }
    temp == test ? cout << "YES" : cout << "NO";
    return 0;
}
