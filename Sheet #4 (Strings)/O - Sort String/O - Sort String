#include <iostream>

using std::cin;
using std::cout;
using std::string;
using std::swap;

int main()
{
    int N;
    cin >> N;
    string S;
    cin >> S;
    for (int i = 0; i < N - 1; i++)
        for (int j = i + 1; j < N; j++)
            if (S[i] > S[j])
                swap(S[i], S[j]);
    cout << S;
    return 0;
}
