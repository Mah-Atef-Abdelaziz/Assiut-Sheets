#include <iostream>

using std::cin;
using std::cout;

int main()
{
    int T, N;
    long long factorial;
    cin >> T;
    for (int i = 0; i < T; i++)
    {
        factorial = 1;
        cin >> N;
        for (int j = N; j > 1; j--)
        {
            factorial *= j;
        }
        cout << factorial << "\n";
    }
    return 0;
}
